from sg_qnsf import QuantumNeuromorphicFabric
from sg_qnsf.config import DomainConfig, FusionMode

# Initialize the universal fabric
fabric = QuantumNeuromorphicFabric(
    domain_config=DomainConfig.UNIVERSAL,
    fusion_mode=FusionMode.ATOMIC,
    security_enabled=True
)

# Start cross-domain intelligence
await fabric.initialize_universal_fabric()

# Process cross-domain data
results = await fabric.process_cross_domain(
    domains=['healthcare', 'climate', 'finance'],
    data_streams=multi_domain_data,
    intelligence_mode='comprehensive'
)

print(f"Fusion Efficiency: {results.fusion_metrics.efficiency}")
print(f"Emergent Capabilities: {results.emergent_capabilities}")
print(f"Cross-Domain Insights: {results.cross_domain_insights}")
